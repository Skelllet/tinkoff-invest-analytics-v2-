<Window x:Name="Main_Window" x:Class="tinkoff_invest_analytics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tinkoff_invest_analytics"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="Operation_Grid" HorizontalAlignment="Left" Height="159" Margin="9,114,0,0" VerticalAlignment="Top" Width="751" d:ItemsSource="{d:SampleData ItemCount=5}"/>
        <Label Content="Совершенные операции" HorizontalAlignment="Left" Margin="5,44,0,0" VerticalAlignment="Top"/>
        <ComboBox HorizontalAlignment="Left" Margin="40,9,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Счет" HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
